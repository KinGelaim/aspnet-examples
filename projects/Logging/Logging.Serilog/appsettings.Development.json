{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "SharedKernel.Infrastructure.Auth": "Warning",
        "Quartz": "Warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Enabled": true,
    "IndexPrefix": "dev-serilog-logging",
    "IndexDateSuffixFormat": "yyyy.MM",
    "Uri": "http://localhost:9200"
  }
}